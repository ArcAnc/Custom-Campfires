{
    "schemaVersion": 1,
    "id": "mod_id",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${description}",
    "authors": [
        "${mod_author}"
    ],
    "contact": {
        "sources": "https://github.com/ArcAnc/Custom-Campfires/issues"
    },
    "license": "${license}",
    "icon": "assets/bc/logo.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "com.arcanc.bc.BeautifulCampfires"
        ]
    },
    "mixins": [
      "bc.mixins.json",
      "bc.fabric.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=0.14",
        "fabric": "*",
        "minecraft": "${minecraft_version}",
        "java": ">=17"
    },
    "suggests": {
        "another-mod": "*"
    }
}
  